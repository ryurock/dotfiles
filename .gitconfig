[user]
    name = ryurock
    email = ryusukekimura3@gmail.com
[color]
    ui = true
[alias]
    graph = log --graph --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    gr = log --graph --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    st = status
    sh = show
    ci = commit
    df = diff
    br = branch
    co = checkout
[core]
    quotepath = false
    editor = /usr/bin/vim
